<form>
  <h1><% vars.title %></h1>
  <h2>Saved games</h2>

  <p class="padding">
    <% IF saves.keys.size == 0 %>
      <strong>Not found any saves</strong>
    <% ELSE %>
      <ul>
      <noindex>
      <% FOREACH pair IN saves.pairs %>
         <li><a href="<% pair.value %>" target="_blank"><% pair.key %></a></li>
      <% END %>
      </noindex>
      </ul>
    <% END %>
  </p>

</form>